/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode(int x) : val(x), next(NULL) {}
 * };
 */
class Solution {
public:
    ListNode* removeNthFromEnd(ListNode* head, int n) {
        ListNode* p=head;
        ListNode* q=head;
        int i;
        if( head -> next == NULL )
        {
            return NULL;
        }
        for( i = 0;i < n ;i++ )
        {    
            q = q -> next;
            if( q == NULL )
                return head->next;
        }
        
        while( q && q -> next )
        {
            p = p -> next;
            q = q -> next;
        }
        p -> next = p -> next -> next;
        return head;
    }
};
