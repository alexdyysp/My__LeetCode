class Solution {
public:
    int findNumbers(vector<int>& nums) {
        int result=0;
        for(int i=0; i<nums.size(); i++){
            int tmp = nums[i];
            int count=0;
            while(tmp != 0){
                tmp /= 10;
                ++count;
            }
            if(count%2==0) result++;
        }
        return result;
    }
};
